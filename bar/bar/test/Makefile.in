	# wrong public key
	@for crypt in $(CRYPT_NAMES); do \
          if test ! $$crypt = "none"; then \
            $(RMF) intermediate/test.bar; \
            $(TEST_ENVIRONMENT) $(TEST_TIMEOUT) $(TEST_BAR_PREFIX) $(TEST_BAR) --crypt-algorithm=$$crypt --crypt-type=asymmetric --crypt-password=$(TEST_PASSWORD_CRYPT) --crypt-public-key=$(CURDIR)/$(TEST_KEY_CRYPT_OTHER_PUBLIC) --skip-verify-signatures -c intermediate/test.bar $(TEST_FILES) $(LOG); \
            rc=$$?; \
            if test $$rc -ne 0; then \
