          $(call functionServerCommandBatch, 304,JOB_ABORT jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 305,JOB_RESET jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 306,JOB_STATUS jobUUID=$(TEST_JOB_UUID)); \
          \
          $(call functionServerCommandBatch, 400,INCLUDE_LIST jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 401,INCLUDE_LIST_CLEAR jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 401,MOUNT_LIST jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 402,MOUNT_LIST_CLEAR jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 403,EXCLUDE_LIST jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 404,EXCLUDE_LIST_CLEAR jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 405,EXCLUDE_COMPRESS_LIST jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 406,EXCLUDE_COMPRESS_LIST_CLEAR jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 407,SOURCE_LIST jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 408,SCHEDULE_LIST jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 409,SOURCE_LIST_CLEAR jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 410,SCHEDULE_LIST_ADD jobUUID=$(TEST_JOB_UUID) archiveType=normal date=*-1-2 weekDays=* time=1:20 interval=300 custoText="test" noStorage=yes enabled=no); \
          $(call functionServerCommandBatch, 411,SCHEDULE_LIST_CLEAR jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 412,PERSISTENCE_LIST_ADD jobUUID=$(TEST_JOB_UUID) archiveType=normal minKeep=1 maxKeep=2 maxAge=3); \
          $(call functionServerCommandBatch, 413,PERSISTENCE_LIST_CLEAR jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch, 414,ARCHIVE_LIST name=$(BAR_STORAGE)intermediate/$(BAR_FILE).bar); \
          \
          $(call functionServerCommandBatch, 500,JOB_OPTION_SET jobUUID=$(TEST_JOB_UUID) name=archive-part-size value=0); \
          \
          $(call functionServerCommandBatch, 600,PAUSE time=300 modeMask=CREATE); \
          $(SLEEP) 2; \
          $(call functionServerCommandBatch, 700,CONTINUE); \
          \
          $(call functionServerCommandBatch, 800,DECRYPT_PASSWORD_CLEAR); \
          $(call functionServerCommandBatch, 801,DECRYPT_PASSWORD_ADD encryptType=NONE encryptedPassword=test); \
          $(call functionServerCommandBatch, 802,FTP_PASSWORD encryptType=NONE encryptedPassword=test); \
          $(call functionServerCommandBatch, 803,SSH_PASSWORD encryptType=NONE encryptedPassword=test); \
          $(call functionServerCommandBatch, 804,WEBDAV_PASSWORD encryptType=NONE encryptedPassword=test); \
          $(call functionServerCommandBatch, 805,CRYPT_PASSWORD jobUUID=$(TEST_JOB_UUID) encryptType=NONE encryptedPassword=test); \
          $(call functionServerCommandBatch, 806,PASSWORDS_CLEAR); \
          \
          $(call functionServerCommandBatch, 900,JOB_DELETE jobUUID=$(TEST_JOB_UUID)); \
          \
          $(call functionServerCommandBatch,1000,STORAGE_LIST_ADD indexIds=1,2,3); \
          $(call functionServerCommandBatch,1001,STORAGE_LIST); \
          $(call functionServerCommandBatch,102,STORAGE_LIST_CLEAR); \
          $(call functionServerCommandBatch,1003,localhost $(TEST_SERVER_PORT),ENTRY_LIST_ADD entryIds=1,2,3); \
          $(call functionServerCommandBatch,1004,localhost $(TEST_SERVER_PORT),ENTRY_LIST); \
          $(call functionServerCommandBatch,1005,localhost $(TEST_SERVER_PORT),ENTRY_LIST_CLEAR); \
          \
          $(call functionServerCommandBatch,1100,INDEX_STORAGE_ADD pattern=$(BAR_STORAGE)intermediate/$(BAR_FILE).bar); \
          $(SLEEP) 2; \
          $(call functionServerCommandBatch,1200,INDEX_UUID_LIST indexStateSet='*' indexModeSet='*' name='*'); \
          \
          $(call functionServerCommandBatch,1300,INDEX_ENTITY_LIST indexStateSet='*' indexModeSet='*' name='*'); \
          \
          $(call functionServerCommandBatch,1400,INDEX_STORAGES_INFO name='*' indexStateSet='*' indexModeSet='*'); \
          $(call functionServerCommandBatch,1400,INDEX_STORAGES_INFO name='test*' indexStateSet='*' indexModeSet='*'); \
          $(call functionServerCommandBatch,1401,INDEX_STORAGE_LIST entityId=* name='*' indexStateSet='*' indexModeSet='*' offset=0 limit=100); \
          $(call functionServerCommandBatch,1401,INDEX_STORAGE_LIST entityId=* name='test*' indexStateSet='*' indexModeSet='*' offset=0 limit=100); \
          \
          $(call functionServerCommandBatch,1500,INDEX_ENTRIES_INFO name='*' indexType='*' newestOnly=yes); \
          $(call functionServerCommandBatch,1501,INDEX_ENTRIES_INFO name='test*' indexType='*' newestOnly=yes); \
          $(call functionServerCommandBatch,1502,INDEX_ENTRY_LIST name='*' indexType='*' newestOnly=yes offset=0 limit=100); \
          $(call functionServerCommandBatch,1503,INDEX_ENTRY_LIST name='test*' indexType='*' newestOnly=yes offset=0 limit=100); \
          \
          $(call functionServerCommandBatch,1600,INDEX_HISTORY_LIST); \
          \
          $(call functionServerCommandBatch,1700,JOB_NEW name=$(TEST_JOB_NAME) jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch,1701,JOB_OPTION_SET jobUUID=$(TEST_JOB_UUID) name=archive-name value=intermediate/$(BAR_FILE).bar); \
          $(call functionServerCommandBatch,1702,JOB_OPTION_SET jobUUID=$(TEST_JOB_UUID) name=archive-type value=normal); \
          $(call functionServerCommandBatch,1703,JOB_OPTION_SET jobUUID=$(TEST_JOB_UUID) name=archive-file-mode value=overwrite); \
          $(call functionServerCommandBatch,1704,INCLUDE_LIST_ADD jobUUID=$(TEST_JOB_UUID) entryType=file pattern=data); \
          $(call functionServerCommandBatch,1704,INCLUDE_LIST_ADD jobUUID=$(TEST_JOB_UUID) entryType=image pattern=$(LOOP0)); \
          $(call functionServerCommandBatch,1705,JOB_FLUSH); \
          $(call functionServerCommandBatch,1706,INCLUDE_LIST jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandBatch,1707,JOB_START jobUUID=$(TEST_JOB_UUID) archiveType=normal); \
          for i in `seq 1 12`; do \
            $(call functionServerCommandBatch,1798,JOB_STATUS jobUUID=$(TEST_JOB_UUID)); \
            $(SLEEP) 10; \
          done; \
          $(call functionServerCommandBatch,1799,JOB_DELETE jobUUID=$(TEST_JOB_UUID)); \
          \
          $(SLEEP) 2; \
          \
          $(call functionServerCommandBatch,1800,DEBUG_PRINT_STATISTICS); \
          $(call functionServerCommandBatch,1801,DEBUG_PRINT_MEMORY_INFO); \
          $(call functionServerCommandBatch,1802,DEBUG_DUMP_MEMORY_INFO); \
          \
          $(call functionServerCommandBatch,9999,QUIT); \
        ) | \
        ( \
          $(ULIMIT) -Sv $(MEMORY_LIMIT); \
          \
          $(INSTALL) -d $(SUB_DIR)/$(TEST_SERVER_JOBS_DIRECTORY); \
          $(RMF) $(SUB_DIR)/$(TEST_SERVER_JOBS_DIRECTORY)/$(TEST_JOB_NAME); \
          $(TEST_ENVIRONMENT) $(TEST_TIMEOUT) $(TEST_BAR_PREFIX) $(TEST_BAR) \
            --index-database=$(TEST_SERVER_DATABASE) \
            --jobs-directory=$(TEST_SERVER_JOBS_DIRECTORY) \
            --log-file=$(TEST_SERVER_LOG_FILE) \
            --log-post-command="cat %file" \
            --log=all \
            --max-threads=1 \
            --batch \
            --server-debug \
            $(ADDITIONAL_OPTIONS) \
            ; \
        ) $(LOG)
	$(RMF) intermediate/$(BAR_FILE).bar
	$(call functionTestDeviceDelete,$(LOOP0))
	$(RMF) $(IMAGE0)
	@$(call functionDoneTestFiles)

.PHONY: test_master_slave_operations test_master_slave_operations_create
test_master_slave_operations: \
  test_master_slave_operations_create \

test_master_slave_operations_master: \
  $(TEST_BAR) \
  $(TEST_MASTER_CONFIG) \
  $(TEST_MASTER_JOBS_DIRECTORY)
	( \
          $(CD) $(UP_DIR); \
          $(ULIMIT) -Sv $(MEMORY_LIMIT); \
          \
          : start master server; \
          $(INSTALL) -d $(SUB_DIR)/$(TEST_MASTER_DIRECTORY); \
          $(INSTALL) -d $(SUB_DIR)/$(TEST_MASTER_JOBS_DIRECTORY); \
          $(INSTALL) -m 600 bar.cfg $(SUB_DIR)/$(TEST_MASTER_CONFIG); \
          $(RMF) $(SUB_DIR)/$(TEST_MASTER_JOBS_DIRECTORY)/$(TEST_JOB_NAME); \
          $(TEST_ENVIRONMENT) $(TEST_TIMEOUT) $(TEST_BAR_PREFIX) $(TEST_BAR) \
            -C $(SUB_DIR) \
            $(TEST_OPTIONS) \
            --config=$(TEST_MASTER_CONFIG) \
            --daemon \
            $(if $(TEST_DETACH),,-D) \
            --pid-file=$(TEST_MASTER_PID_FILE) \
            --server-mode=master \
            --server-port=$(TEST_MASTER_PORT) \
            --index-database=$(TEST_MASTER_DATABASE) \
            --jobs-directory=$(TEST_MASTER_JOBS_DIRECTORY) \
            --log-file=$(TEST_MASTER_LOG_FILE) \
            --log=all \
            --max-threads=1 \
            --server-debug \
            $(ADDITIONAL_OPTIONS) \
            ; \
          if test $$? -eq 0; then \
            $(SLEEP) 10; \
          else \
            exit 1; \
          fi; \
        ) $(LOG)

test_master_slave_operations_slave: \
  $(TEST_BAR) \
  $(TEST_SLAVE_CONFIG) \
  $(TEST_SLAVE_JOBS_DIRECTORY) \
  $(TEST_FILES)
	@$(call functionVerifyParameter,TEST_SLAVE_PORT)
	@$(call functionVerifyParameter,TEST_SLAVE_USERNAME)
	@$(call functionVerifyParameter,TEST_SLAVE_PASSWORD)
	@#
	( \
          $(CD) $(UP_DIR); \
          $(ULIMIT) -Sv $(MEMORY_LIMIT); \
          \
          : start slave server; \
          if test -n "$(TEST_SLAVE_HOST)"; then \
            $(call functionSlaveExecute,$(KILLALL) $(TEST_BAR)) || true; \
            $(call functionSlaveCopy,$(TEST_BAR),$(TEST_BAR)); \
            $(call functionSlaveExecute,$(INSTALL) -d $(TEST_SLAVE_DIRECTORY)); \
            $(call functionSlaveExecute,$(INSTALL) -d $(TEST_SLAVE_JOBS_DIRECTORY)); \
            $(call functionSlaveCopy,bar.cfg,$(TEST_SLAVE_CONFIG)); \
            $(call functionSlaveExecute,$(RMF) $(TEST_SLAVE_JOBS_DIRECTORY)/$(TEST_JOB_NAME)); \
            $(call functionSlaveExecute,$(TEST_ENVIRONMENT) $(TEST_TIMEOUT) $(TEST_BAR_PREFIX) $(TEST_BAR) \
              -C $(SUB_DIR) \
              $(TEST_OPTIONS) \
              --config=$(TEST_SLAVE_CONFIG) \
              --daemon \
              $(if $(TEST_DETACH),,-D) \
              --pid-file=$(TEST_SLAVE_PID_FILE) \
              --server-mode=slave \
              --server-port=$(TEST_SLAVE_PORT) \
              --index-database=$(TEST_SLAVE_DATABASE) \
              --jobs-directory=$(TEST_SLAVE_JOBS_DIRECTORY) \
              --log-file=$(TEST_SLAVE_LOG_FILE) \
              --log-post-command="cat %file" \
              --log=all \
              --max-threads=1 \
              --server-debug \
              $(ADDITIONAL_OPTIONS) \
            ); \
          else \
            $(INSTALL) -d $(SUB_DIR)/$(TEST_SLAVE_DIRECTORY); \
            $(INSTALL) -d $(SUB_DIR)/$(TEST_SLAVE_JOBS_DIRECTORY); \
            $(INSTALL) -m 600 bar.cfg $(SUB_DIR)/$(TEST_SLAVE_CONFIG); \
            $(RMF) $(TEST_SLAVE_JOBS_DIRECTORY)/$(TEST_JOB_NAME); \
            $(TEST_ENVIRONMENT) $(TEST_TIMEOUT) $(TEST_BAR_PREFIX) $(TEST_BAR) \
              -C $(SUB_DIR) \
              $(TEST_OPTIONS) \
              --no-default-config --config=$(TEST_SLAVE_CONFIG) \
              --daemon \
              $(if $(TEST_DETACH),,-D) \
              --pid-file=$(TEST_SLAVE_PID_FILE) \
              --server-mode=slave \
              --server-port=$(TEST_SLAVE_PORT) \
              --index-database=$(TEST_SLAVE_DATABASE) \
              --jobs-directory=$(TEST_SLAVE_JOBS_DIRECTORY) \
              --log-file=$(TEST_SLAVE_LOG_FILE) \
              --log-post-command="cat %file" \
              --log=all \
              --max-threads=1 \
              --server-debug \
              $(ADDITIONAL_OPTIONS); \
          fi; \
          if test $$? -eq 0; then \
            $(SLEEP) 10; \
          else \
            exit 1; \
          fi; \
        ) $(LOG)

test_master_slave_operations_create: \
  $(TEST_BAR)
	@$(call functionVerifyParameter,TEST_BAR)
	@$(call functionVerifyParameter,BAR_FILE)
	@#
	@$(call functionCleanTestFiles)
	@#
	@-$(KILLALL) -9 $(shell basename $(TEST_BAR)) 2>/dev/null
	$(MAKE) test_master_slave_operations_slave TEST_DETACH=1
	$(MAKE) test_master_slave_operations_master TEST_DETACH=1
	@#
	( \
          $(call functionServerCommandSocket,   1,localhost $(TEST_MASTER_PORT),JOB_NEW name=$(TEST_JOB_NAME) jobUUID=$(TEST_JOB_UUID)); \
          $(call functionServerCommandSocket,   2,localhost $(TEST_MASTER_PORT),JOB_OPTION_SET jobUUID=$(TEST_JOB_UUID) name=remote-host-name value=localhost); \
          $(call functionServerCommandSocket,   3,localhost $(TEST_MASTER_PORT),JOB_OPTION_SET jobUUID=$(TEST_JOB_UUID) name=remote-host-port value=$(TEST_SLAVE_PORT)); \
          $(call functionServerCommandSocket,   5,localhost $(TEST_MASTER_PORT),JOB_OPTION_SET jobUUID=$(TEST_JOB_UUID) name=archive-name value=intermediate/$(BAR_FILE).bar); \
          $(call functionServerCommandSocket,   6,localhost $(TEST_MASTER_PORT),JOB_OPTION_SET jobUUID=$(TEST_JOB_UUID) name=archive-type value=normal); \
          $(call functionServerCommandSocket,   6,localhost $(TEST_MASTER_PORT),JOB_OPTION_SET jobUUID=$(TEST_JOB_UUID) name=archive-file-mode value=overwrite); \
          $(call functionServerCommandSocket,   7,localhost $(TEST_MASTER_PORT),INCLUDE_LIST_ADD jobUUID=$(TEST_JOB_UUID) entryType=file pattern=data); \
          $(call functionServerCommandSocket,   8,localhost $(TEST_MASTER_PORT),JOB_FLUSH); \
          \
          $(call functionServerCommandSocket, 100,localhost $(TEST_MASTER_PORT),INCLUDE_LIST jobUUID=$(TEST_JOB_UUID)); \
          \
          $(call functionServerCommandSocket, 200,localhost $(TEST_MASTER_PORT),JOB_START jobUUID=$(TEST_JOB_UUID) archiveType=normal); \
          for i in `seq 1 12`; do \
            $(call functionServerCommandSocket, 300,localhost $(TEST_MASTER_PORT),JOB_STATUS jobUUID=$(TEST_JOB_UUID)); \
            $(SLEEP) 10; \
          done; \
          \
          $(call functionServerCommandSocket,9999,localhost $(TEST_MASTER_PORT),QUIT); \
          $(call functionSlaveCommandSocket, 9999,localhost $(TEST_SLAVE_PORT), QUIT); \
        ) $(LOG)
	@#
	$(call functionWaitProcess,$(TEST_MASTER_PID_FILE),60)
	$(call functionWaitProcess,$(TEST_SLAVE_PID_FILE),60)
	@$(call functionDoneTestFiles)

