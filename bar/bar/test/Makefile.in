	# create test archive
	@$(call functionCleanTestFiles)
	($(CD) $(UP_DIR); $(ULIMIT) -Sv $(MEMORY_LIMIT); $(TEST_ENVIRONMENT) $(TEST_TIMEOUT) $(TEST_BAR_PREFIX) $(TEST_BAR) -C $(SUB_DIR) -c intermediate/$(BAR_FILE).bar $(TEST_FILES) $(BAR_OPTIONS) --overwrite-archive-files --verbose=0 $(LOG))
	@$(call functionTestDeviceReset)
	$(call functionCreateFileZero,$(IMAGE0),32,1M)
	$(call functionTestDeviceCreate,$(LOOP0),$(IMAGE0))
	@#
	# run batch server
	( \
          $(SLEEP) 5; \
