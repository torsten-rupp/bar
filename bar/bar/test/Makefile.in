	($(CD) $(UP_DIR); $(ULIMIT) -Sv $(MEMORY_LIMIT); $(TEST_ENVIRONMENT) $(TEST_TIMEOUT) $(TEST_BAR_PREFIX) $(TEST_BAR) -C $(SUB_DIR) -d intermediate/test*.bar $(BAR_OPTIONS) $(LOG))
	($(CD) $(UP_DIR); $(ULIMIT) -Sv $(MEMORY_LIMIT); $(TEST_ENVIRONMENT) $(TEST_TIMEOUT) $(TEST_BAR_PREFIX) $(TEST_BAR) -C $(SUB_DIR) --convert intermediate/test*.bar $(BAR_OPTIONS) --compress-algorithm=zip9 $(LOG))
	@$(call functionTestDeviceDelete,$(DEVICE0))
	@#
