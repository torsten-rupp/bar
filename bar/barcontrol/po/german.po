msgstr "Speicher die erzeugten Archivdateien auf einem Webdav-Server."

#: src/TabJobs.java:3920
msgid "CD"
msgstr "CD"

#: src/TabJobs.java:3921
msgid "Store created storage files on CD."
msgstr "Archivdateien auf CD erzeugen."

#: src/TabJobs.java:3967
msgid "DVD"
msgstr "DVD"

#: src/TabJobs.java:3968
msgid "Store created storage files on DVD."
msgstr "Speicher die erzeugten Archivdateien auf CD/DVD/BD."

#: src/TabJobs.java:4014
msgid "BD"
msgstr "BD"

