msgstr "Modus"

#: src/TabJobs.java:3538 src/TabJobs.java:9160
msgid "normal"
msgstr "Normal"

#: src/TabJobs.java:3539
msgid "Normal mode: do not create incremental data files."
msgstr "Alle Dateien speichern: keine Inkrementdaten erzeugen."

#: src/TabJobs.java:3561 src/TabJobs.java:9165
msgid "full"
msgstr "voll"

#: src/TabJobs.java:3562
msgid "Full mode: store all entries and create incremental data files."
msgstr "Alle Dateien speichern: Inkrementdaten erzeugen."

#: src/TabJobs.java:3584 src/TabJobs.java:9170
msgid "incremental"
msgstr "Inkrementell"

#: src/TabJobs.java:3585
msgid ""
"Incremental mode: store only modified entries since last full or incremental "
"storage."
msgstr ""
"Inkrementell: speichern von veränderten Dateien seit dem letzten "
"vollständigen oder inkrementellen Speichern."

#: src/TabJobs.java:3607 src/TabJobs.java:9175
msgid "differential"
msgstr "Differentiell"

#: src/TabJobs.java:3608
msgid "Differential mode: store only modified entries since last full storage."
msgstr ""
"Differentiell: speichern von veränderten Dateien seit dem letzten "
"vollständigen Speichern."

#: src/TabJobs.java:3632 src/TabJobs.java:8196
msgid "File name"
msgstr "Gerätename"

#: src/TabJobs.java:3639
msgid ""
"Name of storage files to create. Several macros are supported. Click on "
"button to the right to open storage file name editor."
msgstr ""
"Archivedateiname. Es werden verschiedene Makros unterstützt. Klick auf die "
"Schaltfläche rechts, um den Namenseditor zu öffnen."

#: src/TabJobs.java:3726
msgid "Incremental file name"
msgstr "Inkrementdateiname"

#: src/TabJobs.java:3733
msgid ""
"Name of incremental data file. If no file name is given a name is derived "
"automatically from the storage file name."
msgstr ""
"Name der Inkrementdatei. Falls kein Name angegbeen ist, wird der Name "
"automatishc aus dem Archivnamen abgeleitet."

