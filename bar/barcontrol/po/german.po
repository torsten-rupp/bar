msgid "full"
msgstr "voll"

#: src/TabJobs.java:3582
msgid "Full mode: store all entries and create incremental data files."
msgstr "Alle Dateien speichern: Inkrementdaten erzeugen."

#: src/TabJobs.java:3604 src/TabJobs.java:9486
msgid "incremental"
msgstr "inkrementell"

