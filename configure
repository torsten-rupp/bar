  CXXFLAGS="$CXX_FLAGS -v"
  for z in $CC_DEFINES ""; do
    if test -n "$z"; then
      CPPFLAGS="$CPPFLAGS -D$z"
      CFLAGS="$CFLAGS -D$z"
    fi
  done
  for z in $CC_INCLUDES ""; do
    if test -n "$z"; then
      CPPFLAGS="$CPPFLAGS -I$z"
      CFLAGS="$CFLAGS -I$z"
    fi
  done
  LDFLAGS="$LD_FLAGS"
  for z in $LIBRARY_PATHS ""; do
    if test -n "$z"; then
      LDFLAGS="$LDFLAGS -L$z"
    fi
  done
  LIBS=""
  for z in $STATIC_LIBRARIES ""; do
    if test -n "$z"; then
      LIBS="$LIBS -l$z"
    fi
  done
  for z in $LIBRARIES ""; do
    if test -n "$z"; then
      LIBS="$LIBS -l$z"
    fi
